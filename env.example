# =========================
# Inforadar Pro - env.example
# Реальные значения — только в локальном .env
# =========================

# ===== MYSQL =====
MYSQL_HOST=127.0.0.1
MYSQL_USER=root
MYSQL_PASSWORD=CHANGE_ME
MYSQL_DB=inforadar

# ===== FONBET (Cyprus) PROXY =====
FONBET_PROXY_SERVER=http://IP:PORT
FONBET_PROXY_USERNAME=USER
FONBET_PROXY_PASSWORD=PASS

# ===== FONBET target page (probe) =====
FONBET_TARGET_URL=https://fonbet.com.cy/sports/football?mode=1&dateInterval=5

# ===== FONBET endpoints (после har_extract) =====
FONBET_ENDPOINT_EVENTS=
FONBET_ENDPOINT_EVENT_VIEW=
FONBET_EVENT_ID_PARAM=eventId

# ===== FONBET polling =====
FONBET_BOOKMAKER=fonbet
FONBET_SPORT=football
FONBET_LIVE=0
FONBET_INTERVAL_SEC=10
